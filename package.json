{
  "name": "fireflight",
  "version": "0.1.0",
  "description": "",
  "main": "./build/electron/bootstrap.js",
  "build": {
    "appId": "Fireflight",
    "productName": "Fireflight",
    "files": [
      "./build/**/*",
      "./assets/**/*",
      "**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "npm run clean && npm run start:main && npm run start:renderer && npm run copy:public && npm run start:electron",
    "start:main": "tsc --project tsconfig-electron.json --watch",
    "start:renderer": "node app/scripts/start.js",
    "start:electron": "electron build/electron/bootstrap.js",
    "copy": "npm run copy:public",
    "copy:public": "cp -r app/public build/public",
    "build": "npm run clean && npm run build:renderer && npm run build:main && npm run copy:public",
    "build:renderer": "node app/scripts/build.js",
    "build:main": "tsc --project tsconfig-electron.json",
    "build:electron": "electron-builder --mac",
    "clean": "npm run clean:build && npm run clean:dist",
    "clean:build": "rm -rf build || true",
    "clean:dist": "rm -rf dist || true",
    "eslint": "eslint --ext=ts,tsx,js,jsx .",
    "eslint:fix": "eslint --ext=ts,tsx,js,jsx . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "author": "Ramin Mohammadi",
  "dependencies": {
    "@analytics/google-analytics": "^0.5.0",
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/icons": "^4.2.2",
